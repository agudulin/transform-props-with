{
  "name": "transform-props-with",
  "version": "1.0.0",
  "description": "A props transforming decorator for React components",
  "author": "Robin Pokorny <me@robinpokorny.com> (http://robinpokorny.com)",
  "main": "./lib/index.js",
  "scripts": {
    "compile": "babel src --out-dir lib",
    "prepublish": "npm run compile",
    "pretest": "npm run compile",
    "test": "standard && jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/robinpokorny/transform-props-with"
  },
  "dependencies": {
    "object-assign": "^4.0.1",
    "react": "^0.14.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.3.13",
    "jest-cli": "~0.9.2",
    "react-addons-test-utils": "^0.14.6",
    "react-dom": "^0.14.7",
    "react-stateless-wrapper": "^1.0.2",
    "standard": "^6.0.8"
  },
  "keywords": [
    "transform",
    "props",
    "react",
    "decorator"
  ],
  "license": "MIT",
  "jest": {
    "testDirectoryName": "test",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ]
  }
}
